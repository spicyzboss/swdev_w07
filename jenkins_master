pipeline {
    agent any

    stages {

        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'

                env.name=$(sh "cat 'params.txt' | awk '{ print \$1 }'")
                env.name=$(sh "cat 'params.txt' | awk '{ print \$2 }'")

                build job: 'slave', parameters: [
                    string(name: 'name', value: $name),
                    string(name: 'age', value: $age)
                ]
            }
        }
    }
}