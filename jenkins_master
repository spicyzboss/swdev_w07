pipeline {
    agent any

    stages {

        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'slave', parameters: [
                    string(name: 'name', value: 'Alice'),
                    string(name: 'age', value: '21')
                ]
            }
        }
    }
}